<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Update User</title>
</head>
<body>
    <h1>Update User</h1>
    <h1>Activity Logs</h1>
    <table>
      <tr>
        <th>User ID</th>
        <th>Action</th>
        <th>IP Address</th>
        <th>Timestamp</th>
      </tr>
      <% activityLogs.forEach(log => { %>
        <tr>
          <td><%= log.user_id %></td>
          <td><%= log.action %></td>
          <td><%= log.ip_address %></td>
          <td><%= log.timestamp %></td>
        </tr>
      <% }) %>
    </table>
    
    
</body>
</html>
